{
  "expo": {
    "name": "FactoryOS",
    "slug": "factory-os",
    "version": "1.0.0",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zipybills.factoryos",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Used for scanning QR codes on machines",
        "NSPhotoLibraryUsageDescription": "Used to upload factory photos"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "package": "com.zipybills.factoryos",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "scheme": "factory-os",
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "owner": "zipybills",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e5c4fef9-fc5d-4a78-a68b-d3ef201b5da9"
      }
    }
  }
}
