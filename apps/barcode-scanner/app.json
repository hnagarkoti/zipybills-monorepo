{
  "expo": {
    "name": "barcode-scanner-mobile",
    "slug": "barcode-scanner-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zipybills.barcodescanner"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.zipybills.barcodescanner"
    },
    "plugins": ["expo-router"],
    "scheme": "barcode-scanner",
    "web": {
      "bundler": "metro",
      "output": "single"
    }
  }
}
